{
  "cli": {
    "version": ">= 9.0.7",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_RUNTIME_VERSION": "0.0.1"
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "channel": "ios-simulator",
      "env": {
        "EXPO_RUNTIME_VERSION": "0.0.1"
      }
    },
    "preview": {
      "android": {
        "buildType": "app-bundle"
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_RUNTIME_VERSION": "0.0.1"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_RUNTIME_VERSION": "0.0.1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
